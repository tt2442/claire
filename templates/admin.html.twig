<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta name="robots" content="noindex">

    <title>{% block title %}Administration!{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle`
    and uncomment the following Encore helpers to start using Symfony UX #}
    {% block stylesheets %}
    {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
    {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>

<body>
    {% block header %}
    <nav class="navbar navbar-expand-lg navbar-dark fixed-top bg-dark">
        <a class="nav-link" class="navbar-brand" href="{{path('home')}}">Accueil</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
            aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('accueil_index') }}">Accueil</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('article_index') }}">Articles</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('catalogue_index') }}">Catalogues</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('categorie_index') }}">Categories</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('client_index') }}">Clients</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('collegue_index') }}">Collegues</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('contact_index') }}">Contact</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('createur_index') }}">Createur</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('evenement_index') }}">Événements</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('materiel_index') }}">Materiels</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('newsletter_index') }}">Newsletter</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('realisation_index') }}">Réalisations</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('reseausocial_index') }}">Réseaux sociaux</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('user_index') }}">Utilisateur</a>
                </li>
            </ul>
        </div>
    </nav>
    {% endblock %}
    <div class="container">

        {% block body %}
        {% endblock %}
    </div>

</body>

</html>